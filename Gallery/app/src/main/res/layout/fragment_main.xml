<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:tools="http://schemas.android.com/tools"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             tools:context=".MainFragment">

    <android.support.constraint.ConstraintLayout
            xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:tools="http://schemas.android.com/tools"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:context=".MainFragment">
        <ImageView
                android:layout_width="0dp"
                android:layout_height="0dp" app:srcCompat="@drawable/alpy"
                android:id="@+id/imageView1"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp"
                app:layout_constraintBottom_toTopOf="@+id/imageView2" android:layout_marginBottom="4dp"
                android:layout_marginTop="4dp" android:contentDescription="@string/app_name"/>
        <ImageView
                android:layout_width="0dp"
                app:srcCompat="@drawable/kot"
                android:id="@+id/imageView2" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
                android:layout_height="0dp"
                app:layout_constraintBottom_toTopOf="@+id/imageView3"
                app:layout_constraintTop_toBottomOf="@+id/imageView1" android:layout_marginTop="4dp"
                android:layout_marginBottom="4dp" android:contentDescription="@string/app_name"/>
        <ImageView
                android:layout_width="0dp"
                android:layout_height="0dp" app:srcCompat="@drawable/zaba"
                android:id="@+id/imageView3" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintBottom_toTopOf="@+id/imageView4"
                app:layout_constraintTop_toBottomOf="@+id/imageView2" android:layout_marginTop="4dp"
                android:layout_marginBottom="4dp" android:contentDescription="@string/app_name"/>
        <ImageView
                android:layout_width="0dp"
                android:layout_height="0dp" app:srcCompat="@drawable/pies"
                android:id="@+id/imageView4" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp"
                app:layout_constraintTop_toBottomOf="@+id/imageView3"
                app:layout_constraintBottom_toTopOf="@+id/imageView5" android:layout_marginTop="4dp"
                android:layout_marginBottom="4dp" android:contentDescription="@string/app_name"/>
        <ImageView
                android:layout_width="0dp"
                android:layout_height="0dp" app:srcCompat="@drawable/droga"
                android:id="@+id/imageView5" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp"
                app:layout_constraintTop_toBottomOf="@+id/imageView4" android:layout_marginBottom="4dp"
                app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="4dp"
                android:contentDescription="@string/app_name"/>
    </android.support.constraint.ConstraintLayout>

</FrameLayout>